{
  "name": "cookinity",
  "version": "1.0.0",
  "description": "cookinity",
  "main": "index.js",
  "engines": {
    "node": "14.17.0"
  },
  "scripts": {
    "start-prod": "npm run build && node ./build/index.js",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "start-dev": "node --exec babel-node src/index.js",
    "server": "nodemon --inspect --exec babel-node src/index.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Cookinity Team",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.2.0",
    "config": "^3.3.6",
    "dayjs": "^1.10.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "faker": "^5.5.3",
    "joi": "^17.4.0",
    "joi-oid": "^1.1.2",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.5.2",
    "mongoose": "^5.12.11",
    "mongoose-paginate-v2": "^1.3.18",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "server": "^1.0.33"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
